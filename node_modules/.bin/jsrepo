#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/diojvi/dev/portfolio/node_modules/.pnpm/jsrepo@2.4.9_typescript@5.9.2_zod@3.25.76/node_modules/jsrepo/dist/node_modules:/home/diojvi/dev/portfolio/node_modules/.pnpm/jsrepo@2.4.9_typescript@5.9.2_zod@3.25.76/node_modules/jsrepo/node_modules:/home/diojvi/dev/portfolio/node_modules/.pnpm/jsrepo@2.4.9_typescript@5.9.2_zod@3.25.76/node_modules:/home/diojvi/dev/portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/diojvi/dev/portfolio/node_modules/.pnpm/jsrepo@2.4.9_typescript@5.9.2_zod@3.25.76/node_modules/jsrepo/dist/node_modules:/home/diojvi/dev/portfolio/node_modules/.pnpm/jsrepo@2.4.9_typescript@5.9.2_zod@3.25.76/node_modules/jsrepo/node_modules:/home/diojvi/dev/portfolio/node_modules/.pnpm/jsrepo@2.4.9_typescript@5.9.2_zod@3.25.76/node_modules:/home/diojvi/dev/portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/jsrepo@2.4.9_typescript@5.9.2_zod@3.25.76/node_modules/jsrepo/dist/index.js" "$@"
else
  exec node  "$basedir/../.pnpm/jsrepo@2.4.9_typescript@5.9.2_zod@3.25.76/node_modules/jsrepo/dist/index.js" "$@"
fi
